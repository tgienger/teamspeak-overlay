{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Ultimate Teamspeak",
		"version":"0.1.0",
		"minimum-overwolf-version": "0.77.1.0",
		"author":"Tj Gienger",
		"icon":"images/IconMouseOver.png",
		"icon_gray":"images/IconMouseNormal.png",
		"description":"Custom Teamspeak Overlay"
	},
	"data": {
		"start_window":"index",
		"windows":{
			"index":{
					"file":"index.html",
					"transparent":false,
					"resizable":true,
					"size":{"width":1000, "height":800},
					"min_size":{"width":200, "height":200},
					"max_size":{"width":1000, "height":1000}
				}
		}
	}
}